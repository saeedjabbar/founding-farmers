{
  "collectionName": "components_media_video_embeds",
  "info": {
    "displayName": "Video Embed",
    "description": "Stores iframe embed markup for externally hosted videos"
  },
  "options": {},
  "attributes": {
    "provider": {
      "type": "enumeration",
      "enum": [
        "youtube",
        "vimeo",
        "other"
      ],
      "default": "other"
    },
    "title": {
      "type": "string"
    },
    "embedHtml": {
      "type": "text",
      "required": true
    },
    "aspectRatio": {
      "type": "enumeration",
      "enum": [
        "16:9",
        "4:3",
        "1:1",
        "9:16"
      ],
      "default": "16:9"
    }
  }
}
